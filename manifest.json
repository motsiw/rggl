{
  "manifest_version": 2,
  "name": "Remove German Gender Language",
  "version": "6.1.4",
  "description": "Entfernt deutsche Gender-Sprache weitgehend aus Webseiten.",
  "icons": {
    "64": "button/gendericon_black.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "button/gendericon_black.png",
      "128": "button/gendericon_black_128x128.png"
    },
    "browser_style": true,
    "default_title": "Filterung aktiv"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "removegenderlang.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{53cad060-05d9-4064-a562-69532daddc84}"
    }
  }
}